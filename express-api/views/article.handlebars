{{#each articles}}
    {{#if this.publish}}
        <setcion class="article">
            <h3><a href="{{../backUrl}}">‚ÜêGo back</a></h3>
            <br />

            {{#with this}}
                <h2 class="article-title">
                    {{key}}{{title}}
                </h2>
                <h3 class="article-subTitle">
                    {{subTitle}}
                </h3>
                <h4 class="article-date">
                    {{date}}
                </h4>
                <p class="article-content">
                    {{content}}
                </p>
            {{/with}}
        </setcion>
    {{/if}}
{{/each}}